{
  "compilerOptions": {
    "target": "ES5",
    "allowNonTsExtensions": true,
    "module": "commonjs",
    "sourceMap": true,
    "isolatedModules": true,
    "noEmitOnError": false,
    "rootDir": ".",
    "emitDecoratorMetadata": true,
    "experimentalDecorators": true,
    "out": "./www/build/js/app.bundle.js"
  },
  "exclude": [
    "node_modules",
    "platforms",
    "typings",
    "www/build"
  ]
}
